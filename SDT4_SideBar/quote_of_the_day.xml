<body style="background-color: #F2F2F2">
<table style="border-collapse: collapse;height:94vh;background-color: #F2F2F2;" align="center" border="0" width="100%" bgcolor="#E8E8E8" cellspacing="0" cellpadding="0">
<tr><td align="center" width="100%" height="100%">
<script>
var now = new Date();
var start = new Date(now.getFullYear(), 0, 0);
var diff = now - start;
var oneDay = 1000 * 60 * 60 * 24;
var day = Math.floor(diff / oneDay);
document.write("<i>&ldquo;" + Quotes[day].quote +"&rdquo;</i>"+"<br/>"); 
document.write("- "+ Quotes[day].author);
